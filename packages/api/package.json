{
  "name": "mon-backend",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "watch": "esbuild src/index.ts --watch --bundle --platform=node --target=node16 --external:pg-native --outdir=dist --sourcemap --metafile=dist/meta.json",
    "dev": "concurrently pnpm:watch nodemon",
    "build": "tsc && rimraf dist && esbuild src/index.ts --bundle --minify --platform=node --target=node16 --external:pg-native --outdir=dist --metafile=dist/meta.json",
    "start": "node dist/index.js",
    "analyze": "esbuild-visualizer --metadata ./dist/meta.json --filename ./dist/stats.html",
    "prisma:gen": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:push": "prisma db push"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@google-cloud/pubsub": "^2.19.0",
    "@httpmon/db": "workspace:^0.0.1",
    "@sinclair/typebox": "^0.23.3",
    "@szmarczak/http-timer": "^5.0.1",
    "ajv": "^8.10.0",
    "axios": "^0.25.0",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "emittery": "^0.10.1",
    "fastify": "^3.27.1",
    "fastify-cors": "^6.0.2",
    "fastify-request-context": "^2.2.0",
    "fastify-static": "^4.5.0",
    "firebase-admin": "^10.0.2",
    "fluent-json-schema": "^3.1.0",
    "gcp-metadata": "^5.0.0",
    "got": "11.8.3",
    "handlebars": "^4.7.7",
    "jsonpath-plus": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "kysely": "^0.17.1",
    "lodash.clonedeep": "^4.5.0",
    "nanoid": "^3.3.1",
    "pg": "^8.7.3",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "latest",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.25",
    "esbuild-visualizer": "^0.3.1",
    "fastify-cli": "^2.15.0",
